        <script><![CDATA[/**SNDOC
	@name BR activateCustomer4RTSM
	@description BR on[x_atpu_rtsm_customer_to_function] "after insert or update' with condition active = true"
	
	@see https://
	@domain Alert Management
	@author RTSM
	@version 1.0.0
	*/
	
(function executeRule(current, previous /*null when async*/) {

	var log = new RTSMLog(RTSMLog.LOG_DM_IMPACT_CALC, "activateCustomer4RTSM");
	log.debug(">>> Start BR activateCustomer4RTSM " + RTSMConst.QO.enableCustomerRTSM);
	
	/*
		Recalculates the cached class data in Class To Layer and Model layer
		Necessary in case of release upgrade or defining customer subclasses
	*/
	var irr = new IRRManager();
	irr.populateClasses();

	global.RTSMGlobalScopeUtils.doPostInstallation();

	/*
		Generate Queue Entry
	*/
	new FunnelFiller().insert(RTSMConst.QO.enableCustomerRTSM, current.customer);

	log.debug(">>> End BR activateCustomer4RTSM");
})(current, previous);]]></script>
